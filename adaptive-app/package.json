{
	"name": "adaptive-app",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "prisma generate --no-engine &&  next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "bunx prisma migrate dev",
		"db:migrate": "bunx prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "next dev --turbo",
		"preview": "prisma generate --no-engine && next build && next start",
		"start": "bun run db:migrate && bunx next start",
		"typecheck": "tsc --noEmit",
		"stripe": "stripe listen --forward-to localhost:3000/api/stripe-checkout"
	},
